      font-size 16px
      font-weight normal
      padding-bottom 10px
      border-bottom 1px solid white
    }
    &-bottom {
      @extend .flex
      padding-top 10px
      justify-content space-between
      font-size 24px
    }
  }
  &-order {
    padding: 20px 37px;
    text-transform: uppercase;
    background-image: -webkit-linear-gradient(top,#ef0000,#a30000);
