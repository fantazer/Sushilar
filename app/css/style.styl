.basket-toggle {
  z-index: 100;
  background: white;
  display none
  position absolute
  top 60px
  left 0px
  border-left: 1px solid #b9b9b9;
  border-right: 1px solid #b9b9b9;
  border-bottom: 1px solid #b9b9b9;
  &-toparrow {
    height: 21px;
    border-top: 1px solid transparent;
    border-bottom: 1px solid #b9b9b9;
    &:hover{
      cursor: pointer;
      background-color: #b9b9b9;
    }
    &-item{
      width: 15px;
      height: 8px;
      margin: 6px auto;
      background: url(../img/basket-arrow.png) no-repeat;
    }
  }
  &-item {
    @extend .flex
    font-weight normal
    padding 10px
    border-bottom: 1px solid #b9b9b9;
    &:hover{
      background green
      cursor pointer
      .basket-toggle-item-degree div{
        border 1px solid black
        color black
      }
      .basket-toggle-item-close-ico{
        color black!important
        background: url(../img/delete-item-btn.png) no-repeat !important;
        background-position: -8px 0 !important;
      }
      .basket-toggle-item-close{
        border 1px solid black
      }
    }
    &-val {
      font-size 16px
      margin-right 5px
    }
    &-degree {
      div{
        width 20px
        height 20px
        text-align center
        border 1px solid grey
        color grey
        margin 0px 10px
        &:hover{
          background grey
          color white
          cursor pointer
          
        }
        &:first-child{
          border-bottom 0px!important
        }
      }
    }
    &-text {
      font-size 12px
    }
    &-close {
      border: 1px solid grey
      width 20px
      height 20px  
      border-radius 3px
      float right
      &:hover{
          background red
          border 0px!important
          .basket-toggle-item-close-ico{
            background: url(../img/delete-item-btn.png) no-repeat!important;
            background-position: -16px 0 !important;
            cursor pointer
          }
        }
      @extend .flex
      &-ico{
        width: 8px;
        height: 8px;
        background: url(../img/delete-item-btn.png) no-repeat;

      }
    }
    &-price {
      font-size 20px
      color black
    }
  }
  &-botarrow {
    height: 21px;
    border-top: 1px solid transparent;
    border-bottom: 1px solid #b9b9b9;
    &:hover{
      cursor: pointer;
      background-color: #b9b9b9;
    }
    &-item{
      width: 15px;
      height: 8px;
      margin: 6px auto;
      background: url(../img/basket-arrow.png) 0 -8px no-repeat;
    }
